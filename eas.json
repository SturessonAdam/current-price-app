{
  "cli": {
    "version": ">= 3.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENVIRONMENT": "development"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENVIRONMENT": "production"
      }
    },
    "apkTest": {
      "distribution": "store",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENVIRONMENT": "apkTest"
      }
    }
  }
}

